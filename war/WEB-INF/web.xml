<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>DadesPlayground</servlet-name>
		<servlet-class>com.fisio.main.DadesPlaygroundServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DadesPlayground</servlet-name>
		<url-pattern>/dadesplayground</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>BrezoRodriguez</servlet-name>
		<servlet-class>com.fisio.main.FisioMain</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BrezoRodriguez</servlet-name>
		<url-pattern>/brezoRodriguez</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Vicalmotor</servlet-name>
		<servlet-class>com.fisio.main.Vicalmotor</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Vicalmotor</servlet-name>
		<url-pattern>/vicalmotor</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
